{
  "name": "Phone-project",
  "version": "1.0.0",
  "description": "Standard Project",
  "author": "Agiletech Org",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/common": "7.4.2",
    "@nestjs/core": "7.4.2",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.4.2",
    "@nestjs/platform-socket.io": "^7.6.17",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "4.5.12",
    "@nestjs/typeorm": "7.1.0",
    "@nestjs/websockets": "^7.6.17",
    "bcrypt": "5.0.0",
    "bluebird": "3.7.2",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "form-data": "^4.0.0",
    "generate-password": "1.5.1",
    "gm": "1.23.1",
    "js-sha256": "^0.9.0",
    "moment": "2.27.0",
    "mysql": "2.18.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "6.4.11",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pino": "^6.11.3",
    "pino-multi-stream": "5.3.0",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.2",
    "typeorm": "0.2.32",
    "typeorm-seeding": "1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.4.2",
    "@types/bcrypt": "3.0.0",
    "@types/bluebird": "3.5.32",
    "@types/cron": "1.7.2",
    "@types/express": "4.17.7",
    "@types/faker": "5.5.4",
    "@types/fluent-ffmpeg": "2.1.17",
    "@types/gm": "^1.18.9",
    "@types/jest": "26.0.10",
    "@types/lodash": "4.14.159",
    "@types/multer": "1.4.5",
    "@types/mysql": "2.15.15",
    "@types/node": "14.6.0",
    "@types/passport-jwt": "3.0.3",
    "@types/pino": "6.3.8",
    "@types/pino-multi-stream": "5.1.1",
    "@types/redis": "2.8.25",
    "@types/supertest": "2.0.10",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "26.4.0",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "swagger-ui-express": "4.1.4",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
